{
  "expo": {
    "name": "AwesomeProject",
    "slug": "AwesomeProject",
    "scheme": "AwesomeProject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "permissions": {
        "PHOTOS": "Allow access to your photos"
      },
      "bundleIdentifier": "com.nathencinoza.AwesomeProject"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA_ROLL"
      ],
      "package": "com.nathencinoza.AwesomeProject"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
